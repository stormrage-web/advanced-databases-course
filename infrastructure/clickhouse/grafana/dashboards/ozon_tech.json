        {
          "refId": "A",
          "expr": "rate(ClickHouseProfileEvents_Query[1m])",
          "legendFormat": "query/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Select QPS (rate(SelectQuery))",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(ClickHouseProfileEvents_SelectQuery[1m])",
          "legendFormat": "select/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory resident (bytes)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
